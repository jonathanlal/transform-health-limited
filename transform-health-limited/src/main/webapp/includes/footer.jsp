        <script>
        
        document.getElementById("btnDemoSubmit").onclick = function() {
        	if(validateDemoForm()){submitDemo()}
        };
        function validateDemoForm() {
        	var check = false;
        	var e=document.getElementById("demail").value,n=/^\w+@[a-zA-Z_]+?\.[a-zA-Z]{2,3}$/;
        	if(!e.match(n)){
        		errorDemoInput("demail");
        	}else{check=true;}
        	if(document.getElementById("dname").value == '' || document.getElementById("dname").value.length == 0){
        		errorDemoInput("dname");
        		check=false;
        	}else{check=true;}
        	return check;
        }
        function resetDemoErrorInput(){
        	changeDemoPlaceHolderTextByElementID("dname");
            var name = document.getElementById("dname");
            name.classList.remove("errorInput");
            changeDemoPlaceHolderTextByElementID("demail");
            var email = document.getElementById("demail");
            email.classList.remove("errorInput");
        }
        function changeDemoPlaceHolderTextByElementID(elementID){
        	document.getElementById(elementID).placeholder=getTextFromDemoNode(elementID+'txt');
        }
        function getTextFromDemoNode(elementID){
        	var node = document.getElementById(elementID);
        	return node.textContent || node.innerText;
        }
        function errorDemoInput(elementID){
        	 var element = document.getElementById(elementID);
        	 element.placeholder=getDemoValidationText(elementID);
        	 element.className += " errorInput";
        	 window.setTimeout(resetDemoErrorInput, 3000);
        }
        function getDemoValidationText(elementID){
        	var text;
        		if(elementID.indexOf('dname')>=0){text = "*Type in your name.";}
        		if(elementID.indexOf('demail')>=0){text = "*Type in your email.";}
        	return text;
        }
        function submitDemo(){
        	var fmessage = document.getElementById('dmessage').value;
        	var fname = document.getElementById('dname').value;
        	var femail = document.getElementById('demail').value;
        	var phone = document.getElementById('dphone').value;
        	var url = window.location.href;
          document.getElementById("demosent").style.display = "block";
          document.getElementById("democontainer").style.display = "none";
        	var e = "name="+fname+"&email="+femail+"&msg="+fmessage+"&phone="+phone+"&url="+url;
        	var n=new XMLHttpRequest();
        	n.open("POST","../../../ContactServlet",!0),
        	n.setRequestHeader("Content-type","application/x-www-form-urlencoded"),
        	n.onreadystatechange=function(){
        		  if (n.readyState == 4) {
        			  document.getElementById("demosent").style.display = "block";
        			  document.getElementById("democontainer").style.display = "none";
        	        }
        	},n.send(e)
        }

